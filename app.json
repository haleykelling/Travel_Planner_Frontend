{
  "expo": {
    "name": "travel-planner-frontend",
    "slug": "travel-planner-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/globe.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flatiron.wanderlust"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
